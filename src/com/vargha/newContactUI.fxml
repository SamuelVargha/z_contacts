<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="169.0" prefWidth="395.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vargha.NewContactController">
      <Text layoutX="14.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add a contact:">
         <font>
            <Font size="17.0" />
         </font>
      </Text>
      <TextField fx:id="nameField" layoutX="14.0" layoutY="65.0" onKeyTyped="#nameKey" prefWidth="170.0" AnchorPane.topAnchor="65.0" />
      <TextField fx:id="mailField" layoutX="14.0" layoutY="119.0" onKeyTyped="#mailKey" prefWidth="170.0" AnchorPane.topAnchor="119.0" />
      <TextField fx:id="phoneField" layoutX="205.0" layoutY="65.0" onKeyTyped="#phoneKey" prefWidth="170.0" AnchorPane.topAnchor="65.0" />
      <Text layoutX="14.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:" />
      <Text layoutX="205.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone Number:" />
      <Text layoutX="14.0" layoutY="115.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email:" />
      <Button fx:id="okButton" layoutX="300.0" layoutY="119.0" minHeight="20.0" minWidth="75.0" mnemonicParsing="false" onAction="#ok" text="Ok" />
      <Button layoutX="215.0" layoutY="119.0" minHeight="20.0" minWidth="75.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
</AnchorPane>
